{
    "buildCommand": "hugo --gc --minify",
    "installCommand": "wget -O - https://github.com/gohugoio/hugo/releases/download/v0.123.6/hugo_extended_0.123.6_Linux-64bit.tar.gz | tar -xz",
    "framework": null,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
} 